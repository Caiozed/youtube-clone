<%= form_for @video, url: path do |f|%>
    <%= f.label :name %>
    <%= f.text_field :name %>
    
    <%= f.label :description %>
    <%= f.text_area :description%>
    
    <%= f.label :thumbnail %>
    <%= f.file_field :thumbnail%>
    
    <%= f.label :video_file%>
    <%= f.file_field :video_file %>
    <%= f.submit "Upload video"%>
<%end%>